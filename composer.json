{
    "name": "crwlr/url",
    "description": "Swiss Army knife for urls.",
    "keywords": [
        "crwlr",
        "url",
        "urls",
        "uri",
        "parser",
        "parse",
        "parsing",
        "components",
        "idn",
        "rfc3986",
        "rfc3987",
        "psr-7",
        "uriinterface"
    ],
    "homepage": "https://www.crwlr.software/packages/url",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Olear",
            "homepage": "https://www.otsch.codes",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~7.1",
        "psr/http-message": "^1.0",
        "symfony/polyfill-intl-idn": "^1.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Crwlr\\Url\\": "src"
        }
    },
    "scripts": {
        "test": "@php vendor/bin/phpunit",
        "cs": "@php vendor/bin/php-cs-fixer fix -v --diff --dry-run --allow-risky=yes",
        "update-suffixes": "@php bin/update-suffixes",
        "update-schemes": "@php bin/update-schemes"
    }
}
